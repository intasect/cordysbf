<binding-template>
	<beans>
		<bean beanid="default" class="com.cordys.coe.bf.content.types.coboc.folders.Template" />
	</beans>
	
	<handlers>
		<handler type="coboctuple" xmlpath="." keepchildren="true"  />
		<handler type="bean" beanid="default" xmlpath="@lastModified" fieldname="lastModified" params="skip-insert" />
		<handler type="bean" beanid="default" xmlpath="@key" fieldname="key"  />
		<handler type="bean" beanid="default" xmlpath="@name" fieldname="name"  />
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/DESCRIPTION" fieldname="description"  />
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/ENTITY_NAME" fieldname="name"  />
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/PERMANENTLY_INMEMORY" fieldname="permanentlyInMemory"  />		
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/PERSIST_INDB" fieldname="persistsInDb"  />	
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/VALIDATE_SCHEMA" fieldname="validateSchema"  />			
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/VALIDATE_RULE" fieldname="validateRule"  />	
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/ICONURL" fieldname="iconUrl"  />		
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/REFERENCE" fieldname="reference"  />	
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/HISTORY" fieldname="history"  />			
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/PLUGINCLASS" fieldname="pluginClass"  />			
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/XSD_SCHEMA" fieldname="schema" xmlfield="true" />
	</handlers>
	
	<template>
		<tuple lastModified="" key="" version="organization" name="">
	        <new>
	          <ENTITY>
	            <DESCRIPTION/>
	            <ENTITY_NAME></ENTITY_NAME>
	            <PERMANENTLY_INMEMORY></PERMANENTLY_INMEMORY>
	            <PERSIST_INDB></PERSIST_INDB>
	            <VALIDATE_SCHEMA></VALIDATE_SCHEMA>
	            <VALIDATE_RULE></VALIDATE_RULE>
	            <ICONURL/>
	            <REFERENCE/>
	            <HISTORY></HISTORY>
	            <PLUGINCLASS></PLUGINCLASS>
	            <XSD_SCHEMA>
	            </XSD_SCHEMA>
	          </ENTITY>
        	</new>
        </tuple>
	</template>
</binding-template>