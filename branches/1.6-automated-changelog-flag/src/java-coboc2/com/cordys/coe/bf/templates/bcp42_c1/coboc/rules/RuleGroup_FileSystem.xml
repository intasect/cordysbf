<binding-template>
	<beans>
		<bean beanid="default" class="com.cordys.coe.bf.content.types.coboc.rules.RuleGroup" />
	</beans>
	
	<handlers>
		<handler type="bean" beanid="default" xmlpath="rulegroupname" fieldname="name"  />
		<handler type="bean" beanid="default" xmlpath="orgcontext" fieldname="organization"  />
		<handler type="bean" beanid="default" xmlpath="rulegroupowner" fieldname="owner"  />
		<handler type="bean" beanid="default" xmlpath="rulegroupdescription" fieldname="description"  />		
		<handler type="bean" beanid="default" xmlpath="rulegrouppriority" fieldname="priority"  />				
	</handlers>
	
	<template>
		<rulegroup>
		  <rulegroupid></rulegroupid>
		  <orgcontext></orgcontext>
		  <rulegroupname></rulegroupname>
		  <rulegroupowner></rulegroupowner>
		  <rulegroupdescription/>
		  <rulegrouppriority></rulegrouppriority>
		</rulegroup>
	</template>
</binding-template>