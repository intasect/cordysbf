<binding-template>
	<beans>
		<bean beanid="default" class="com.cordys.coe.bf.content.types.coboc.mdm.Backend" />
	</beans>
	
	<handlers>
		<handler type="bean" beanid="default" xmlpath="@key" fieldname="key"  />
		<handler type="bean" beanid="default" xmlpath="@name" fieldname="name"  />
		<handler type="bean" beanid="default" xmlpath="@entity_id" fieldname="objectId"  />
		<handler type="bean" beanid="default" xmlpath="@customkey" fieldname="objectId"  />
		<handler type="bean" beanid="default" xmlpath="@entity_type" fieldname="templateId"  />
		<handler type="bean" beanid="default" xmlpath="@lastModified" fieldname="lastModified"  />	
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/ENTITY_ID" fieldname="objectId"  />			
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/PARENT_ID" fieldname="parentId"  />
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/ORGANIZATION" fieldname="organization"  />
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/OWNER" fieldname="owner"  />
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/DESCRIPTION" fieldname="description"  />
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/EVENT_FLAG" fieldname="eventFlag"  />
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/HISTORY" fieldname="history"  />			
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/CUSTOMKEY" fieldname="objectId"  />	
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/TEMPLATE_ID" fieldname="templateId"  />	
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/OBJECT/backend/documentProperties" fieldname="documentProperties" xmlfield="true"/>
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/OBJECT/backend/content/identity" fieldname="identity" />		
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/OBJECT/backend/content/displayname" fieldname="displayName" />
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/OBJECT/backend/content/publisherdn" fieldname="publisherDn" />
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/OBJECT/backend/content/backendtype" fieldname="backendType" />
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/OBJECT/backend/content/ismdm" fieldname="isMdm" />
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/OBJECT/backend/content/sniffer" fieldname="sniffer" xmlfield="true" />		
	</handlers>
	
	<template>
		<tuple lastModified="" key="" name="" entity_id="" entity_type="" customkey="" level="organization">
	        <old>
	          <ENTITY>
	            <ENTITY_ID></ENTITY_ID>
	            <PARENT_ID></PARENT_ID>
	            <ORGANIZATION></ORGANIZATION>
	            <OWNER></OWNER>
	            <DESCRIPTION></DESCRIPTION>
	            <EVENT_FLAG></EVENT_FLAG>
	            <HISTORY></HISTORY>
	            <CUSTOMKEY></CUSTOMKEY>
	            <TEMPLATE_ID></TEMPLATE_ID>
	            <OBJECT>
	              <backend>
	                <documentProperties>
	                  <name></name>
	                  <description/>
	                  <caption/>
	                  <mimeType/>
	                  <notes/>
	                  <version/>
	                  <revision/>
	                  <createdBy></createdBy>
	                  <creationDate/>
	                  <lastModifiedBy></lastModifiedBy>
	                  <lastModificationDate/>
	                </documentProperties>
	                <content>
	                  <identity></identity>
	                  <displayname></displayname>
	                  <publisherdn/>
	                  <backendtype></backendtype>
	                  <ismdm></ismdm>
	                  <sniffer>
	                    <type></type>
	                    <trigger>
	                      <mdmuser/>
	                    </trigger>
	                    <auditreaders/>
	                  </sniffer>
	                </content>
	              </backend>
	            </OBJECT>
	          </ENTITY>
	        </old>
	      </tuple>
	</template>
</binding-template>