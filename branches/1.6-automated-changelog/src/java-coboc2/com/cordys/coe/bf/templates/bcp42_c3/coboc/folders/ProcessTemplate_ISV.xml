<binding-template>
	<beans>
		<bean beanid="default" class="com.cordys.coe.bf.content.types.coboc.folders.ProcessTemplate" />
	</beans>
	
	<handlers>
		<handler type="xml" xmlpath="." fieldname="processName" params="keep-ns" />
		<handler type="bean" beanid="default" xmlpath="bizprocess[1]/processname" fieldname="processName" keepchildren="true" />
		<handler type="bean" beanid="default" xmlpath="bizprocess[1]/processmodel/processinstance" fieldname="processTemplateData" xmlfield="true" params="keep-ns" />
		<handler type="bean" beanid="default" xmlpath="bizprocess[2]/processname" fieldname="bpmn.key" keepchildren="true" />
		<handler type="bean" beanid="default" xmlpath="bizprocess[2]/processmodel/bpmtuplewsdl" fieldname="bpmn.processBpmnData" xmlfield="true" params="keep-ns" />
	</handlers>
	
	<template>
		<process xmlns="">
        	<bizprocess xmlns="http://schemas.cordys.com/bpm/deployment/1.0">
          		<processname modelSpace="organization"></processname>
          		<processmodel>
		        	<processinstance>
		            </processinstance>
		        </processmodel>       
	        </bizprocess>
			<bizprocess xmlns="http://schemas.cordys.com/bpm/deployment/1.0">
          		<processname modelSpace="organization"></processname>
          		<processmodel>
            		<bpmtuplewsdl>
            		</bpmtuplewsdl>
            	</processmodel>          		
          	</bizprocess>	        
	    </process>
	</template>
</binding-template>