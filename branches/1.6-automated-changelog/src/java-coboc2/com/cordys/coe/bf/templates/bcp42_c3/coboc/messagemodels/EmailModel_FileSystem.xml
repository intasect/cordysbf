<binding-template>
	<imports>
		<template file="EmailModel_Common.xml" template-id="common" destpath="email-model" />
	</imports>
	
	<beans>
		<bean beanid="default" class="com.cordys.coe.bf.content.types.coboc.messagemodels.EmailModel" />
	</beans>
	
	<handlers>
		<handler type="property" propname="bf-version" xmlpath="@bf-version"  />
		<handler type="bean" beanid="default" xmlpath="@original-id" fieldname="originalObjectId" />		
		<import-handlers template-id="common" />
		<handler type="bean" beanid="default" xmlpath="@name" fieldname="name" />
		<handler type="bean" beanid="default" xmlpath="@parentKey" fieldname="parentKey" />
	</handlers>
	
	<template>
      	<email-model bf-version=""  original-id="" name="" parentKey="">
		</email-model>
	</template>
</binding-template>