<?xml version="1.0" encoding="UTF-8"?>
<soapnode
	name="Local Coboc Service"
	description="">
	<namespace>http://schemas.cordys.com/1.0/coboc</namespace>
	<namespace>http://schemas.cordys.com/4.2/coboc</namespace>
	<namespace>http://schemas.cordys.com/1.0/log</namespace>
	<namespace>http://schemas.cordys.com/bpm/process</namespace>
	<methodset>cn=COBOC Method Set 4.2,cn=Cordys Orchestrator 4.2,cn=cordys,o=vanenburg.com</methodset>
	<methodset>cn=COBOC Method Set,cn=Cordys Orchestrator 4.2,cn=cordys,o=vanenburg.com</methodset>
	<methodset>cn=Log Method Set,cn=Cordys Orchestrator 4.2,cn=cordys,o=vanenburg.com</methodset>
	<methodset>cn=Methodset Mapping,cn=method sets,o=TLA,cn=cordys,o=vanenburg.com</methodset>
	<soapprocessor
		name="Local Coboc Processor"
		description=""
		computer="srv-nl-demo11"
		osprocesshost=""
		automaticstart="true">
		<soapprocessorconfiguration>
			<configurations
				autoStartCount="3">
				<cancelReplyInterval>30000</cancelReplyInterval>
				<gracefulCompleteTime>15</gracefulCompleteTime>
				<abortTime>5</abortTime>
				<jreconfig>
					<param
						value="-Xmx64M"
					/>
				</jreconfig>
				<routing
					ui_type="loadbalancing"
					ui_algorithm="failover">
					<preference>1</preference>
				</routing>
				<loggerconfiguration>
					<systempolicy>true</systempolicy>
					<log4j:configuration
						xmlns:log4j="http://jakarta.apache.org/log4j">
						<renderer
							renderedClass="com.eibus.util.logger.internal.LocalizableLogMessage"
							renderingClass="com.eibus.util.logger.internal.TextRenderer"
						/>
						<renderer
							renderedClass="com.eibus.util.logger.internal.LogMessage"
							renderingClass="com.eibus.util.logger.internal.TextRenderer"
						/>
						<root>
							<priority
								value="error"
							/>
							<appender-ref
								ref="DailyRollingFileAppender"
							/>
						</root>
						<category
							name="com.eibus.security.acl">
							<priority
								value="error"
							/>
						</category>
						<category
							name="com.eibus.license">
							<priority
								value="error"
							/>
						</category>
						<category
							name="com.eibus.directory">
							<priority
								value="error"
							/>
						</category>
						<category
							name="com.eibus.soap">
							<priority
								value="error"
							/>
						</category>
						<category
							name="com.eibus.transport.SOAPMessage">
							<priority
								value="error"
							/>
						</category>
						<category
							name="com.eibus.transport">
							<priority
								value="error"
							/>
						</category>
						<category
							name="com.eibus.tools">
							<priority
								value="error"
							/>
						</category>
						<category
							name="com.eibus.util">
							<priority
								value="error"
							/>
						</category>
						<category
							name="Cordys.Orchestrator.All">
							<priority
								value="error"
							/>
						</category>
						<category
							name="Cordys.Parser.Service">
							<priority
								value="error"
							/>
						</category>
						<category
							name="Cordys.Parser.Library">
							<priority
								value="error"
							/>
						</category>
						<appender
							name="DailyRollingFileAppender"
							class="org.apache.log4j.DailyRollingFileAppender">
							<param
								name="File"
								value="tla#tla_coboc_service#tla_coboc_service_processor.xml"
							/>
							<param
								name="DatePattern"
								value=".yyyy-MM-dd"
							/>
							<layout
								class="org.apache.log4j.xml.XMLLayout">
								<param
									name="locationInfo"
									value="true"
								/>
							</layout>
						</appender>
					</log4j:configuration>
				</loggerconfiguration>
				<spyPublish>false</spyPublish>
				<spyFile/>
				<spyLogger/>
				<spyLevels/>
				<spyCategories/>
				<configuration
					implementation="com.cordys.cpc.coboc.net.COBOCWCPMapper"
					htmfile="/cordys/cpc/developer/content/coboctabpage.htm">
					<classpath
						xmlns="http://schemas.cordys.com/1.0/xmlstore">
						<location>cpc/bin/cpc.jar</location>
						<location>cpc/bin/cpc_trang.jar</location>
						<location>cpc/bin/castor-0.9.4.3-xml.jar</location>
						<location>cpc/bin/bsf.jar</location>
						<location>cpc/bin/js.jar</location>
						<location>cpc/bin/xalan.jar</location>
					</classpath>
					<coboc>
						<property>
							<key>coboc.hashbins</key>
							<value>10</value>
						</property>
						<property>
							<key>coboc.hashbins.capacity</key>
							<value>1000</value>
						</property>
						<property>
							<key>coboc.object.idletime</key>
							<value>1</value>
						</property>
						<property>
							<key>coboc.dbuser</key>
							<value>root</value>
						</property>
						<property>
							<key>coboc.encodedPassword</key>
							<value>true</value>
						</property>
						<property>
							<key>coboc.dbpassword</key>
							<value>UGFzc3cwcmQ=</value>
						</property>
						<property>
							<key>coboc.defaultDB</key>
							<value>CORDYS</value>
						</property>
						<property>
							<key>coboc.dbdatasource</key>
							<value>srv-nl-demo11</value>
						</property>
						<property>
							<key>coboc.os</key>
							<value>windows</value>
						</property>
						<property>
							<key>coboc.XQYDBdriver</key>
							<value>OLEDB</value>
						</property>
						<property>
							<key>coboc.dbprovider</key>
							<value>SQLNCLI</value>
						</property>
						<property>
							<key>coboc.querycachesize</key>
							<value>100</value>
						</property>
						<property>
							<key>coboc.querycacheRefInterval</key>
							<value>3600</value>
						</property>
						<property>
							<key>coboc.cursorcachesize</key>
							<value>1000</value>
						</property>
						<property>
							<key>coboc.cursorcacheRefInterval</key>
							<value>3600</value>
						</property>
						<property>
							<key>coboc.updateconnections</key>
							<value>3</value>
						</property>
						<property>
							<key>coboc.readconnections</key>
							<value>5</value>
						</property>
						<property>
							<key>coboc.tracelevel</key>
							<value>255</value>
						</property>
						<property>
							<key>coboc.coboceventdisplayurl</key>
							<value>/cordys/cpc/cobocinbox/cobocmessages.htm</value>
						</property>
						<property>
							<key>coboc.coboceventdisplayid</key>
							<value>cid</value>
						</property>
						<property>
							<key>coboc.coboceventdisplayframe</key>
							<value>main</value>
						</property>
						<property>
							<key>coboc.notificationservice</key>
							<value/>
						</property>
						<property>
							<key>coboc.event.enable</key>
							<value>true</value>
						</property>
						<property>
							<key>coboc.rules.enable</key>
							<value>true</value>
						</property>
						<property>
							<key>coboc.schedule.enable</key>
							<value>false</value>
						</property>
						<property>
							<key>coboc.processflow.enable</key>
							<value>true</value>
						</property>
						<property>
							<key>coboc.network</key>
							<value>true</value>
						</property>
						<property>
							<key>coboc.adminservice</key>
							<value>true</value>
						</property>
						<property>
							<key>coboc.security.enable</key>
							<value>true</value>
						</property>
						<property>
							<key>flowengine.enablebinaryparser</key>
							<value>false</value>
						</property>
						<property>
							<key>flowengine.numthreads</key>
							<value>5</value>
						</property>
						<property>
							<key>flowengine.crashrecovery</key>
							<value>true</value>
						</property>
						<property>
							<key>flowengine.activatedebugpoints</key>
							<value>true</value>
						</property>
						<property>
							<key>flowengine.priorityenabled</key>
							<value>true</value>
						</property>
						<property>
							<key>flowengine.priorityalgo</key>
							<value>com.cordys.cpc.flowengine.priorityqueue.algorithm.AgingAlgorithm</value>
						</property>
						<property>
							<key>flowengine.enableaging</key>
							<value>true</value>
						</property>
						<property>
							<key>flowengine.agingtime</key>
							<value>3600</value>
						</property>
					</coboc>
					<component
						name="Business Object Cache">
						<numCoBOCHashbins>10</numCoBOCHashbins>
						<hashbinsCapacity>1000</hashbinsCapacity>
					</component>
				</configuration>
			</configurations>
		</soapprocessorconfiguration>
		<connectionpoint
			name="connectionpoint-Local CoBOC Processor"
			description=""
			private="false"
			labeleduri="socket://srv-nl-demo11:62658"
		/>
	</soapprocessor>
</soapnode>
