<binding-template>
	<beans>
		<bean beanid="default" class="com.cordys.coe.bf.content.types.coboc.schedules.ScheduleTemplate" />
	</beans>

	<handlers>
		<handler type="bean" beanid="default" xmlpath="old/SCHEDULETEMPLATE/TEMPLATEID" fieldname="objectId" />
		<handler type="bean" beanid="default" xmlpath="old/SCHEDULETEMPLATE/TEMPLATENAME" fieldname="name"  />
		<handler type="property" xmlpath="old/SCHEDULETEMPLATE/SCHEDULEXML" propname="SCHEDULEXML" />
		<handler type="bean" beanid="default" xmlpath="old/SCHEDULETEMPLATE/LASTUPDATED" fieldname="lastModified" params="skip-insert" />
	</handlers>
		
	<template>
		<tuple xmlns="http://schemas.cordys.com/1.0/coboc">
        	<old>
       			<SCHEDULETEMPLATE>
         			<TEMPLATEID></TEMPLATEID>
         			<TEMPLATENAME></TEMPLATENAME>
         			<SCHEDULEXML></SCHEDULEXML>
			        <LASTUPDATED></LASTUPDATED>
			        <OWNER>unknown</OWNER>
       			</SCHEDULETEMPLATE>
       		</old>
       	</tuple>
	</template>
</binding-template>