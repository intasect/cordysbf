<binding-template>
	<beans>
		<bean beanid="default" class="com.cordys.coe.bf.content.types.ldap.MethodSet" />
	</beans>
	
	<handlers>
		<handler type="bean" beanid="default" xmlpath="@dn" fieldname="ldapDn"  />
		<handler type="bean" beanid="default" xmlpath="cn/string" fieldname="name"  />
		<handler type="bean" beanid="default" xmlpath="labeleduri/string" fieldname="namespace"  />
		<handler type="bean" beanid="default" xmlpath="implementationclass/string" fieldname="implementationClass"  />
	</handlers>
	
	<template>
		<entry
			dn="cn=Method Set AppLinxDummy,cn=method sets,o=testing,cn=cordys,o=vanenburg.com">
			<cn>
				<string>Method Set AppLinxDummy</string>
			</cn>
			<objectclass>
				<string>top</string>
				<string>busmethodset</string>
			</objectclass>
			<labeleduri>
				<string>http://www.amev.nl/amev/</string>
			</labeleduri>
			<implementationclass>
				<string>
					com.eibus.applicationconnector.java.JavaCallHandler
				</string>
			</implementationclass>
		</entry>
	</template>
</binding-template>