<binding-template>
	<imports>
		<template file="EmailModel_Common.xml" template-id="common" destpath="tuple/new" />
	</imports>
	
	<beans>
		<bean beanid="default" class="com.cordys.coe.bf.content.types.coboc.messagemodels.EmailModel" />
	</beans>
	
	<handlers>
		<handler type="coboctuple" xmlpath="." keepchildren="true"  />
		<import-handlers template-id="common" />
		<handler type="bean" beanid="default" xmlpath="@key" fieldname="detailKey" />
		<handler type="bean" beanid="default" xmlpath="@name" fieldname="name" />		
		<handler type="bean" beanid="default" xmlpath="@lastModified" fieldname="detailLastModified" params="skip-insert" />
	</handlers>
	
	<template>
      	<tuple lastModified="" key="" level="organization" name="">
        	<new>
        	</new>
		</tuple>
	</template>
</binding-template>