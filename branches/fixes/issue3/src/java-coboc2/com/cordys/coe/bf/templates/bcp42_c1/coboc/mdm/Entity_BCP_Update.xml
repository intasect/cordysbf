<binding-template>
	<!--  Used only for updates -->
	<beans>
		<bean beanid="default" class="com.cordys.coe.bf.content.types.coboc.mdm.Entity" />
	</beans>
	
	<handlers>
		<handler type="coboctuple" xmlpath="." keepchildren="true"  />
		<handler type="bean" beanid="default" xmlpath="@key" fieldname="key"  />
		<handler type="bean" beanid="default" xmlpath="@template" fieldname="templateKey"  />
		<handler type="bean" beanid="default" xmlpath="@name" fieldname="name"  />
		<handler type="bean" beanid="default" xmlpath="@lastModified" fieldname="lastModified" params="skip-insert" />	
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/ENTITY_ID" fieldname="objectId"  />	
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/ENTITY_NAME" fieldname="name"  />		
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/DESCRIPTION" fieldname="description"  />
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/OBJECT/entity/documentProperties" fieldname="documentProperties" xmlfield="true"/>
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/OBJECT/entity/content/backendname" fieldname="backendId" />		
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/OBJECT/entity/content/displayname" fieldname="displayName" />
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/OBJECT/entity/content/identity" fieldname="identity" />		
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/OBJECT/entity/content/templatelocation" fieldname="templateLocation" />
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/OBJECT/entity/content/templateversion" fieldname="templateVersion" />
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/OBJECT/entity/content/audittablename" fieldname="auditTableName" />
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/OBJECT/entity/content/scheduleinterval" fieldname="scheduleInterval" />
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/OBJECT/entity/content/getmethod" fieldname="getMethod" />
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/OBJECT/entity/content/pullmethod" fieldname="pullMethod" />
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/OBJECT/entity/content/updatemethod" fieldname="updateMethod" />
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/OBJECT/entity/content/keyfields" fieldname="keyFields" />	
	</handlers>
	
	<template>
		<tuple key="" template="" version="organization" lastModified="" name="">
			<old>
				<ENTITY>
					<ENTITY_ID></ENTITY_ID>
				</ENTITY>
			</old>
			<new>
				<ENTITY>
					<ENTITY_NAME></ENTITY_NAME>
					<DESCRIPTION></DESCRIPTION>
					<OBJECT>
						<entity>
							<documentProperties></documentProperties>
							<content>
								<backendname></backendname>
								<displayname></displayname>
								<identity></identity>
								<templatelocation></templatelocation>
								<templateversion></templateversion>
								<audittablename />
								<scheduleinterval />
								<getmethod></getmethod>
								<pullmethod />
								<updatemethod></updatemethod>
								<keyfields></keyfields>
							</content>
						</entity>
					</OBJECT>
				</ENTITY>
			</new>
		</tuple>
	</template>
</binding-template>