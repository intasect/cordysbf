<binding-template>
	<beans>
		<bean beanid="default" class="com.cordys.coe.bf.content.types.ldap.MethodXsd" />
	</beans>
	
	<handlers>
		<handler type="bean" beanid="default" xmlpath="@dn" fieldname="ldapDn"  />
		<handler type="bean" beanid="default" xmlpath="cn/string" fieldname="name"  />
		<handler type="bean" beanid="default" xmlpath="busmethodtypexsd/string" fieldname="xsd"  />
	</handlers>
	
	<template>
		<entry
			dn="cn=CustomerCustomerDemo.xsd,cn=Method Set Northwind,cn=Cordys Northwind Demo 1.0,cn=cordys,o=vanenburg.com">
			<cn>
				<string>CustomerCustomerDemo.xsd</string>
			</cn>
			<objectclass>
				<string>top</string>
				<string>busmethod</string>
				<string>busmethodtype</string>
			</objectclass>
			<busmethodtypexsd>
				<string></string>
			</busmethodtypexsd>
		</entry>
	</template>
</binding-template>