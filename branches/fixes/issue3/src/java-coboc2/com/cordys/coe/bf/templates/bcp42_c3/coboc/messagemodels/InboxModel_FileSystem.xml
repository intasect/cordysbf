<binding-template>
	<imports>
		<template file="InboxModel_Common.xml" template-id="common" destpath="inbox-model" />
	</imports>
	
	<beans>
		<bean beanid="default" class="com.cordys.coe.bf.content.types.coboc.messagemodels.InboxModel_C3" />
	</beans>
	
	<handlers>
		<handler type="property" propname="bf-version" xmlpath="@bf-version"  />
		<handler type="bean" beanid="default" xmlpath="@original-id" fieldname="originalObjectId" />
		<import-handlers template-id="common" />
		<handler type="bean" beanid="default" xmlpath="MESSAGE_MODEL/MODEL_ID" fieldname="key" />
		<handler type="bean" beanid="default" xmlpath="MESSAGE_MODEL/MESSAGE_TEMPLATE_ID" fieldname="parentKey" />
		<remove-handler xmlpath="MESSAGE_MODEL/LAST_MODIFIED" />		
		<remove-handler xmlpath="MESSAGE_MODEL/ORGANIZATION" />		
	</handlers>
	
	<template>
      	<inbox-model bf-version="" original-id="">
		</inbox-model>
	</template>
</binding-template>