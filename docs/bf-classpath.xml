<libraries source="sdk">
	<library>
		<name>CoE Library</name>
		<description>Java utility method library provided by Cordys CoE</description>
		<jars>
			<jar>
				<location>sdk/lib/coelib.jar</location>
				<javadoc type="jar:platform">
					<location>sdk/docs/coelib-javadocs.zip!/</location>
				</javadoc>
			</jar>
		</jars>
	</library>
	
	<library>
		<name>Apache Commons Collections</name>
		<description>Extends or augments the Java Collections Framework</description>
		<jars>	
			<jar>
				<location>sdk/lib/commons/commons-collections-3.2.jar</location>
				<match-pattern>^sdk/lib/commons/commons-collections-.*</match-pattern>
				<javadoc>
					<location>http://commons.apache.org/collections/api-release</location>
				</javadoc>					
			</jar>		
		</jars>
	</library>
	
	<library>
		<name>Apache Commons Lang</name>
		<description>Provides extra functionality for classes in java.lang</description>
		<jars>	
			<jar>
				<location>sdk/lib/commons/commons-lang-2.2.jar</location>
				<match-pattern>^sdk/lib/commons/commons-lang-.*</match-pattern>
				<javadoc>
					<location>http://commons.apache.org/lang/api-release</location>
				</javadoc>				
			</jar>			
		</jars>
	</library>
	
	<library>
		<name>Apache Commons Net</name>
		<description>Collection of network utilities and protocol implementations</description>
		<jars>	
			<jar>
				<location>sdk/lib/commons/commons-net-1.4.1.jar</location>
				<match-pattern>^sdk/lib/commons/commons-net-.*</match-pattern>
				<javadoc>
					<location>http://commons.apache.org/net/apidocs</location>
				</javadoc>					
			</jar>			
		</jars>
	</library>
	
	<library>
		<name>Apache Commons CLI</name>
		<description>Command Line arguments parser</description>
		<jars>	
			<jar>
				<location>sdk/lib/commons/commons-cli-1.0.jar</location>
				<match-pattern>^sdk/lib/commons/commons-cli-.*</match-pattern>
				<javadoc>
					<location>http://commons.apache.org/cli/api-release</location>
				</javadoc>					
			</jar>			
		</jars>
	</library>
	
	<library>
		<name>Apache Commons Configuration</name>
		<description>Reading of configuration/preferences files in various formats</description>
		<jars>	
			<jar>
				<location>sdk/lib/commons/commons-configuration-1.3.jar</location>
				<match-pattern>^sdk/lib/commons/commons-configuration-.*</match-pattern>
				<javadoc>
					<location>http://commons.apache.org/configuration/apidocs</location>
				</javadoc>					
			</jar>			
		</jars>
	</library>	
</libraries>
