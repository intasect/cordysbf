<binding-template templateid="COBOC-GetRootFolderContents">
	<handlers>
		<handler type="property" propname="VERSION" xmlpath="Body/GetCollection/folder/@version"  />
	</handlers>
	
	<template>
		<SOAP:Envelope xmlns:SOAP="http://schemas.xmlsoap.org/soap/envelope/" >
		  <SOAP:Body>
		    <GetCollection xmlns="http://schemas.cordys.com/4.2/coboc">
		      <folder filter="folder" type="entity" version="organization"/>
		    </GetCollection>
		  </SOAP:Body>
		</SOAP:Envelope>
	</template>
</binding-template>