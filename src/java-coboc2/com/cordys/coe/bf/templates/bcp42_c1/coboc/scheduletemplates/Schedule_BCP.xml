<binding-template>
	<beans>
		<bean beanid="default" class="com.cordys.coe.bf.content.types.coboc.schedules.ScheduleTemplate" />
	</beans>
	
	<handlers>
		<handler type="bean" beanid="default" xmlpath="@templateid" fieldname="objectId" params="skip-insert" />
		<handler type="bean" beanid="default" xmlpath="@name" fieldname="name"  />
		<handler type="bean" beanid="default" xmlpath="@count" fieldname="count"  />
		<handler type="bean" beanid="default" xmlpath="@lastupdated" fieldname="lastModified" params="skip-insert" />
		<handler type="bean" beanid="default" xmlpath="@autodeploy" fieldname="autodeploy" params="skip-empty" />
		<handler type="bean" beanid="default" xmlpath="datetime" fieldname="datetime" xmlfield="true" />
		<handler type="bean" beanid="default" xmlpath="listener" fieldname="listener"  />
		<handler type="bean" beanid="default" xmlpath="parameters" fieldname="parameters" xmlfield="true" />
	</handlers>
	
	<template>
		<schedule name="" count="" templateid="" lastupdated="" autodeploy="">
           <datetime>
           </datetime>
           <listener></listener>
           <parameters>
           </parameters>
         </schedule>
	</template>
</binding-template>
