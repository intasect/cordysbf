<binding-template>
	<handlers>
		<handler type="tuple" xmlpath="Body/Update/tuple/old" />
		<handler type="tuple" xmlpath="Body/Update/tuple/new" />
	</handlers>
	
	<template>
		<SOAP:Envelope xmlns:SOAP="http://schemas.xmlsoap.org/soap/envelope/">
			<SOAP:Body>
				<Update xmlns="http://schemas.cordys.com/1.0/ldap">
					<tuple>
						<old></old>
						<new></new>
					</tuple>
				</Update>
			</SOAP:Body>
		</SOAP:Envelope>
	</template>
</binding-template>		