<binding-template templateid="COBOC-CreateTemplateSpecialAttributes">
	<beans>
		<bean beanid="new" class="com.cordys.coe.bf.content.types.coboc.folders.SpecialAttribute" />
	</beans>
	
	<handlers>
		<handler type="bean" beanid="new" xmlpath="Body/Create/tuple/new/attribute_template/attribute_name" fieldname="name"  />
		<handler type="bean" beanid="new" xmlpath="Body/Create/tuple/new/attribute_template/template_id" fieldname="parentId"  />
		<handler type="bean" beanid="new" xmlpath="Body/Create/tuple/new/attribute_template/attribute_type" fieldname="attributeType"  />
		<handler type="bean" beanid="new" xmlpath="Body/Create/tuple/new/attribute_template/primary_key" fieldname="primaryKey"  />
	</handlers>
	
	<template>
		<SOAP:Envelope xmlns:SOAP="http://schemas.xmlsoap.org/soap/envelope/" >
		  	<SOAP:Body>	
				<Create xmlns="http://schemas.cordys.com/1.0/coboc" table="attribute_template">
					<tuple>
				    	<new>
					        <attribute_template>
					        	<attribute_name></attribute_name>
					           	<template_id></template_id>
					            <attribute_type></attribute_type>
					            <primary_key></primary_key>
					        </attribute_template>				    	
				    	</new>
				  	</tuple>
				</Create>	
			  </SOAP:Body>
		</SOAP:Envelope>
	</template>
</binding-template>