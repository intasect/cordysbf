<binding-template templateid="COBOC-GetSubFolderContents">
	<handlers>
		<handler type="property" propname="FOLDER_KEY" xmlpath="Body/GetCollection/folder"  />
		<handler type="property" propname="VERSION" xmlpath="Body/GetCollection/folder/@version"  />
	</handlers>
	
	<template>
		<SOAP:Envelope xmlns:SOAP="http://schemas.xmlsoap.org/soap/envelope/" >
		  <SOAP:Body>
		    <GetCollection xmlns="http://schemas.cordys.com/4.2/coboc">
		      <folder filter="folder" type="entity" version="organization"></folder>
		    </GetCollection>
		    <GetCollection xmlns="http://schemas.cordys.com/4.2/coboc">
		      <folder filter="template" type="entity" version="organization"></folder>
		    </GetCollection>
		    <GetCollection xmlns="http://schemas.cordys.com/4.2/coboc">
		      <folder filter="/MAP" type="instance" version="organization"></folder>
		    </GetCollection>
		    <GetCollection xmlns="http://schemas.cordys.com/4.2/coboc">
		      <folder filter="/PROCESSTEMPLATE" type="instance" version="organization"></folder>
		    </GetCollection>
		    <GetCollection xmlns="http://schemas.cordys.com/4.2/coboc">
		      <folder filter="/CONTENTMAP" type="instance" version="organization"></folder>
		    </GetCollection>
		    <GetCollection xmlns="http://schemas.cordys.com/4.2/coboc">
		      <folder filter="/DECISIONCASE" type="instance" version="organization"></folder>
		    </GetCollection>
		    <GetCollection xmlns="http://schemas.cordys.com/4.2/coboc">
		      <folder filter="/CONDITION" type="instance" version="organization"></folder>
		    </GetCollection>
		    <GetCollection xmlns="http://schemas.cordys.com/4.2/coboc">
		      <folder filter="/ACTION" type="instance" version="organization"></folder>
		    </GetCollection>
		  </SOAP:Body>
		</SOAP:Envelope>
	</template>		
</binding-template>	