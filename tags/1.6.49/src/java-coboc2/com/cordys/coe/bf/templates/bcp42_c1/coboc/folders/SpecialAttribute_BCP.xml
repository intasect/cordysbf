<binding-template>
	<beans>
		<bean beanid="default" class="com.cordys.coe.bf.content.types.coboc.folders.SpecialAttribute" />
	</beans>
	
	<handlers>
		<handler type="bean" beanid="default" xmlpath="attribute_name" fieldname="name"  />
		<handler type="bean" beanid="default" xmlpath="template_id" fieldname="parentId"  />
		<handler type="bean" beanid="default" xmlpath="attribute_type" fieldname="attributeType"  />
		<handler type="bean" beanid="default" xmlpath="primary_key" fieldname="primaryKey"  />
		<handler type="bean" beanid="default" xmlpath="attribtemplate_id" fieldname="objectId" params="skip-insert" />
	</handlers>
	
	<template>
        <attribute_template>
        	<attribute_name></attribute_name>
           	<template_id></template_id>
            <attribute_type></attribute_type>
            <primary_key></primary_key>
            <attribtemplate_id></attribtemplate_id>
        </attribute_template>
	</template>
</binding-template>