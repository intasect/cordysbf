<binding-template>
	<imports>
		<template file="MessageModel_Common.xml" template-id="common" destpath="message-model" />
	</imports>
	
	<beans>
		<bean beanid="default" class="com.cordys.coe.bf.content.types.coboc.messagemodels.InboxModel_C1" />
	</beans>
	
	<handlers>
		<handler type="property" propname="bf-version" xmlpath="@bf-version"  />
		<import-handlers template-id="common" />
		<remove-handler xmlpath="MESSAGE_MODEL/MODEL_ID" />
		<remove-handler xmlpath="MESSAGE_MODEL/LAST_MODIFIED" />		
	</handlers>
	
	<template>
      	<message-model bf-version="">
		</message-model>
	</template>
</binding-template>