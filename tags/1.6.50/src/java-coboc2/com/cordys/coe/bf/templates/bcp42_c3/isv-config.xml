<config extends="../bcp42_c1/isv-config.xml">
    <binding-templates>
        <!--  CoBOC Folders Bindings -->
        <template path="coboc/folders/ActionTemplate_BCP.xml" content-type="COBOC_FOLDERS_ACTIONTEMPLATE" />      
        <template path="coboc/folders/ProcessTemplate_ISV.xml" content-type="COBOC_FOLDERS_PROCESSTEMPLATE" />      
        <template path="coboc/folders/ProcessBpml_BCP.xml" content-type="COBOC_FOLDERS_PROCESSBPML" />      
        <template path="coboc/folders/ProcessBpmn_BCP.xml" content-type="COBOC_FOLDERS_PROCESSBPMN" />
    
        <!--  CoBOC Rule Bindings -->
        <template path="coboc/rules/Rule_BCP.xml" content-type="COBOC_RULES_RULE" />
        
        <!--  CoBOC Message Model Bindings -->
        <template path="coboc/messagemodels/MessageTemplate_ISV.xml" content-type="COBOC_MESSAGE_TEMPLATE" />
        <template path="coboc/messagemodels/InboxModel_ISV.xml" content-type="COBOC_INBOX_MODEL_C3" />
        <template path="coboc/messagemodels/EmailModel_ISV.xml" content-type="COBOC_EMAIL_MODEL" />
    </binding-templates>
    
	<write-methods>
    	<method>
    		<content-types>COBOC_FOLDERS,MDM,COBOC_RULES,COBOC_SCHEDULES,COBOC_MESSAGE_MODELS</content-types>
    		<class>com.cordys.coe.bf.content.coboc.impl.bcp42_c3.IsvWriteMethod_Coboc</class>
    	</method>
    </write-methods>       
</config>