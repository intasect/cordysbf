<bcp-versions>
	<!-- 
		This file list patterns to find out the right BCP major version.
		The destination values are constants in the EBcpVersion enumeration. 
	-->
	
	<default version="BCP42_C3" fixpack="1" buildnumber="338" />
	
	<!-- C3 Builds -->	
	<match version="BCP42_C3" fixpack="unknown">
		<buildVersionRegex fixpack-group="1">^C3\.(\d+)(:?.*)?$</buildVersionRegex>
	</match>		
	<match version="BCP42_C3" fixpack="">
		<buildVersionRegex>^4\.2C3$</buildVersionRegex>
		<buildNumber start="0" end="276"/>
	</match>	
	
	<!-- C2 Builds -->
	<match version="BCP42_C2" fixpack="7">
		<buildVersionRegex>^4\.2$</buildVersionRegex>
		<buildNumber start="605" end=""/>
	</match>	
	<match version="BCP42_C2" fixpack="6">
		<buildVersionRegex>^4\.2$</buildVersionRegex>
		<buildNumber start="588" end="604"/>
	</match>		
	<match version="BCP42_C2" fixpack="5">
		<buildVersionRegex>^4\.2$</buildVersionRegex>
		<buildNumber start="573" end="587"/>
	</match>	
	<match version="BCP42_C2" fixpack="4">
		<buildVersionRegex>^4\.2$</buildVersionRegex>
		<buildNumber start="539" end="572"/>
	</match>	
	<match version="BCP42_C2" fixpack="3">
		<buildVersionRegex>^4\.2$</buildVersionRegex>
		<buildNumber start="509" end="538"/>
	</match>	
	<match version="BCP42_C2" fixpack="2">
		<buildVersionRegex>^4\.2$</buildVersionRegex>
		<buildNumber start="426" end="508"/>
	</match>			
	<match version="BCP42_C2" fixpack="1">
		<buildVersionRegex>^4\.2$</buildVersionRegex>
		<buildNumber start="408" end="425"/>
	</match>
	<match version="BCP42_C2" fixpack="">
		<buildVersionRegex>^4\.2$</buildVersionRegex>
		<buildNumber start="171" end="407"/>
	</match>
		
	<!-- C1 Builds -->
	<match version="BCP42_C1" fixpack="">
		<buildVersionRegex>^4\.2$</buildVersionRegex>
		<buildNumber start="133" end="170"/>
	</match>
</bcp-versions>