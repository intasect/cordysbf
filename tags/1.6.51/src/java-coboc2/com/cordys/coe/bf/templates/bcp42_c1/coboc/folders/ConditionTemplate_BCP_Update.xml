<binding-template>
	<!--  Used only for updates -->
	<beans>
		<bean beanid="default" class="com.cordys.coe.bf.content.types.coboc.folders.ConditionTemplate" />
	</beans>
	
	<handlers>
		<handler type="coboctuple" xmlpath="." keepchildren="true"  />
		<handler type="bean" beanid="default" xmlpath="@key" fieldname="key"  />
		<handler type="bean" beanid="default" xmlpath="@name" fieldname="name"  />
		<handler type="bean" beanid="default" xmlpath="@lastModified" fieldname="lastModified" params="skip-insert" />	
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/ENTITY_ID" fieldname="objectId"  />			
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/ENTITY_NAME" fieldname="name"  />		
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/DESCRIPTION" fieldname="description"  />
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/OBJECT/Condition/ConditionTemplateName" fieldname="name" />
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/OBJECT/Condition/ConditionTemplateDescription" fieldname="conditionTemplateDescription" />
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/OBJECT/Condition/ConditionRepositoryName" fieldname="parentKey"  />
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/OBJECT/Condition/ConditionRepositoryId" fieldname="parentId" />
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/OBJECT/Condition/ConditionRepositoryVersion" fieldname="conditionRepositoryVersion"  />		
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/OBJECT/Condition/TableData" fieldname="tableData" xmlfield="true"  />		
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/OBJECT/Condition/ConditionExpression" fieldname="conditionExpression" />		
		<handler type="bean" beanid="default" xmlpath="new/ENTITY/OBJECT/Condition/Parameters" fieldname="parameters" xmlfield="true" />
	</handlers>
	
	<template>
		<tuple key="" template="/CONDITION" version="organization" lastModified="" name="">
	        <old>
	          <ENTITY>
	            <ENTITY_ID></ENTITY_ID>
	          </ENTITY>
	        </old>
	        <new>
	          <ENTITY>
	            <ENTITY_NAME></ENTITY_NAME>
	            <DESCRIPTION></DESCRIPTION>
	            <OBJECT>
	              <Condition>
	                <ConditionTemplateName></ConditionTemplateName>
	                <ConditionTemplateDescription></ConditionTemplateDescription>
	                <ConditionRepositoryName></ConditionRepositoryName>
	                <ConditionRepositoryVersion></ConditionRepositoryVersion>
	                <ConditionRepositoryId></ConditionRepositoryId>
	                <TableData>
	                </TableData>
	                <ConditionExpression></ConditionExpression>
	                <Parameters>
	                </Parameters>
	              </Condition>
	            </OBJECT>
	          </ENTITY>
	        </new>
	      </tuple>
	</template>
</binding-template>