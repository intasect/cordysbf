<binding-template>
	<imports>
		<template file="MessageTemplate_Common.xml" template-id="common" destpath="tuple/new" />
	</imports>
	
	<beans>
		<bean beanid="default" class="com.cordys.coe.bf.content.types.coboc.messagemodels.MessageTemplate" />
	</beans>
	
	<handlers>
		<handler type="coboctuple" xmlpath="." keepchildren="true"  />
		<import-handlers template-id="common" />
		<handler type="bean" beanid="default" xmlpath="old/messageTemplate/messagetemplateid" fieldname="objectId" params="skip-insert" />
		<handler type="bean" beanid="default" xmlpath="old/messageTemplate/lastmodified" fieldname="lastModified" />
	</handlers>
	
	<template>
      	<tuple>
        	<old>
	          	<messageTemplate>
           			<messagetemplateid></messagetemplateid>
            		<lastmodified></lastmodified>
          		</messageTemplate>
        	</old>
        	<new>
    	    </new>	
		</tuple>
	</template>
</binding-template>