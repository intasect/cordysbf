<?xml version="1.0" encoding="UTF-8"?>
<!-- Menu content are stored in this xml -->
<!-- 'menus' is the root tag here -->
<menus>
	<!-- Every menu is enclosed within the <menu>...</menu> tag -->
	<menu key="/Cordys/WCP/Menu/Helloworld" name="Helloworld">
		<!-- menu related data -->
		<Application
			focus="true"
			display="visible"
			taskbar="true"
			toolbar="false"
			title="true"
			menu="true">
			<description>Hello World</description>
			<id>HelloWorldApp</id>
			<caption>Hello World</caption>
			<url>/cordys/CordysHelloWorld/html/hello.htm</url>
			<frame
				docked="true">main</frame>
			<icon/>
			<openIcon/>
			<data/>
		</Application>
	</menu>
	<menu key="/Cordys/WCP/Menu/Helloworld/Test" name="Helloworld" file="${src.xmlstore}/menus/TestMenu.xml" />
</menus>