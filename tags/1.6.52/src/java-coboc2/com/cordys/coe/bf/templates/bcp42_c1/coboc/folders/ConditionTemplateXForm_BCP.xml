<binding-template>
	<beans>
		<bean beanid="default" class="com.cordys.coe.bf.content.types.coboc.folders.ConditionTemplateXForm" />
	</beans>
	
	<handlers>
		<handler type="bean" beanid="default" xmlpath="@key" fieldname="key"  />
		<handler type="bean" beanid="default" xmlpath="@name" fieldname="name"  />
		<handler type="bean" beanid="default" xmlpath="@lastModified" fieldname="lastModified"  />
		<handler type="bean" beanid="default" xmlpath="@level" fieldname="level"  />
		<handler type="bean" beanid="default" xmlpath="old" fieldname="xformContent" xmlfield="true" />	
	</handlers>
	
	<template>
		<tuple lastModified="" key="" name="" level="">
	        <old>
	        </old>
	      </tuple>
	</template>
</binding-template>