<binding-template>
	<!--  This is an abstract template for all CoBOC instances -->
	
	<handlers>
		<handler type="property" propname="bf-version" xmlpath="@bf-version"  />
		<handler type="bean" beanid="default" xmlpath="@key" fieldname="key"  />
		<handler type="bean" beanid="default" xmlpath="@name" fieldname="name"  />
		<handler type="bean" beanid="default" xmlpath="@customkey" fieldname="key"  />
		<handler type="bean" beanid="default" xmlpath="@entity_type" fieldname="templateKey"  />
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/ENTITY_KEY" fieldname="key"  />
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/PARENT_KEY" fieldname="parentKey"  />
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/ORGANIZATION" fieldname="organization"  />
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/OWNER" fieldname="owner"  />
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/DESCRIPTION" fieldname="description"  />
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/EVENT_FLAG" fieldname="eventFlag"  />
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/HISTORY" fieldname="history"  />			
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/CUSTOMKEY" fieldname="key"  />	
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/TEMPLATE_KEY" fieldname="templateKey"  />	
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/OBJECT" fieldname="instanceObject" xmlfield="true" />
	</handlers>
	
	<template>
		<tuple key="" 
				name="" 
				entity_id=""
				entity_type="" 
				customkey="" 
				level="organization"
				FILE_TYPE="template-instance"
				bf-version="">
	        <old>
	          <ENTITY>
	            <ENTITY_KEY></ENTITY_KEY>
	            <PARENT_KEY></PARENT_KEY>
	            <ORGANIZATION></ORGANIZATION>
	            <OWNER></OWNER>
	            <DESCRIPTION></DESCRIPTION>
	            <EVENT_FLAG>0</EVENT_FLAG>
	            <HISTORY>0</HISTORY>
	            <CUSTOMKEY></CUSTOMKEY>
	            <TEMPLATE_KEY></TEMPLATE_KEY>
	            <OBJECT>
	            </OBJECT>
	          </ENTITY>
	        </old>
	      </tuple>
	</template>
</binding-template>