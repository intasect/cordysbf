<binding-template>
	<beans>
		<bean beanid="default" class="com.cordys.coe.bf.content.types.coboc.folders.SpecialAttribute" />
	</beans>
	
	<handlers>
		<handler type="bean" beanid="default" xmlpath="old/attribute_template/attribute_name" fieldname="name"  />
		<handler type="bean" beanid="default" xmlpath="old/attribute_template/attribute_type" fieldname="attributeType"  />
		<handler type="bean" beanid="default" xmlpath="old/attribute_template/primary_key" fieldname="primaryKey"  />
	</handlers>
	
	<template>
		<tuple>
			<old>
		        <attribute_template>
		        	<attribute_name></attribute_name>
		            <attribute_type></attribute_type>
		            <primary_key></primary_key>
		        </attribute_template>
		    </old>
		</tuple>
	</template>
</binding-template>