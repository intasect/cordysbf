<binding-template templateid="LDAP-GetChildren">
	<handlers>
		<handler type="property" propname="PARENT_DN" xmlpath="Body/GetChildren/dn"  />
	</handlers>
	
	<template>
		<SOAP:Envelope xmlns:SOAP="http://schemas.xmlsoap.org/soap/envelope/">
			<SOAP:Body>
				<GetChildren xmlns="http://schemas.cordys.com/1.0/ldap">
					<dn></dn>
					<sort>ascending</sort>
				</GetChildren>
			</SOAP:Body>
		</SOAP:Envelope>
	</template>
</binding-template>		