<binding-template>
	<beans>
		<bean beanid="default" class="com.cordys.coe.bf.content.types.coboc.folders.Template" />
	</beans>
	
	<handlers>
		<handler type="property" propname="bf-version" xmlpath="@bf-version"  />
		<handler type="bean" beanid="default" xmlpath="@key" fieldname="key"  />
		<handler type="bean" beanid="default" xmlpath="@name" fieldname="name"  />
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/PARENT_KEY" fieldname="parentKey"  />
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/DESCRIPTION" fieldname="description"  />
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/ORGANIZATION" fieldname="organization"  />
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/OWNER" fieldname="owner"  />
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/XSD_SCHEMA" fieldname="schema" xmlfield="true" />
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/PERMANENTLY_INMEMORY" fieldname="permanentlyInMemory"  />		
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/VALIDATE_SCHEMA" fieldname="validateSchema"  />					
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/ICONURL" fieldname="iconUrl"  />		
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/REFERENCE" fieldname="reference"  />	
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/VALIDATE_RULE" fieldname="validateRule"  />	
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/PERSIST_INDB" fieldname="persistsInDb"  />	
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/HISTORY" fieldname="history"  />			
		<handler type="bean" beanid="default" xmlpath="old/ENTITY/PLUGINCLASS" fieldname="pluginClass"  />			
	</handlers>
	
	<template>
		<tuple
			key=""
			name=""
			level="organization"
			FILE_TYPE="template"
			bf-version="">
	        <old>
	          <ENTITY>
	            <PARENT_KEY></PARENT_KEY>
	            <ORGANIZATION></ORGANIZATION>
	            <OWNER></OWNER>
	            <DESCRIPTION/>
	            <XSD_SCHEMA>
	            </XSD_SCHEMA>
	            <PERMANENTLY_INMEMORY>0</PERMANENTLY_INMEMORY>
	            <VALIDATE_SCHEMA>1</VALIDATE_SCHEMA>
	            <ICONURL/>
	            <REFERENCE/>
	            <VALIDATE_RULE>1</VALIDATE_RULE>
	            <PERSIST_INDB>1</PERSIST_INDB>
	            <HISTORY>0</HISTORY>
	            <PLUGINCLASS>DEFAULT</PLUGINCLASS>
	          </ENTITY>
	        </old>
	      </tuple>
	</template>
</binding-template>