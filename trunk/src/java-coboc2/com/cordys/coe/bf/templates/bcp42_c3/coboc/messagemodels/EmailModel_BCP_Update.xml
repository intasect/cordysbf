<binding-template>
	<beans>
		<bean beanid="default" class="com.cordys.coe.bf.content.types.coboc.messagemodels.EmailModel" />
	</beans>
	
	<handlers>
		<handler type="coboctuple" xmlpath="." keepchildren="true"  />
		<handler type="bean" beanid="default" xmlpath="old/EmailModel/EmailModelId" fieldname="objectId" />
		<handler type="bean" beanid="default" xmlpath="old/EmailModel/LastModified" fieldname="lastModified" />
		<handler type="bean" beanid="default" xmlpath="new/EmailModel/EmailModelId" fieldname="objectId" />
		<handler type="bean" beanid="default" xmlpath="new/EmailModel/MessageTemplateId" fieldname="parentId" />
		<handler type="bean" beanid="default" xmlpath="new/EmailModel/EmailModelName" fieldname="name" />
	</handlers>
	
	<template>
      	<tuple>
          <old>
            <EmailModel>
              <EmailModelId></EmailModelId>
              <LastModified></LastModified>
            </EmailModel>
          </old>
          <new>
            <EmailModel>
              <EmailModelId></EmailModelId>
              <MessageTemplateId></MessageTemplateId>
              <EmailModelName></EmailModelName>
            </EmailModel>
          </new>
		</tuple>
	</template>
</binding-template>